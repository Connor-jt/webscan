{
    "name": "obj ext",
    "description": "my ext",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./res/icon/16.png",
        "32": "./res/icon/32.png",
        "48": "./res/icon/48.png",
        "128": "./res/icon/128.png"
    },
    "browser_action": {
        "default_icon": "./res/icon/128.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["./js/background.js"]
    },
    "permissions": [
        "<all_urls>",
        "tabs"
    ]
}